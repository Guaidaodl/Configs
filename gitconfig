[alias]
	st = status
	br = branch
	cm = commit -am
	l2 = log -2
	l5 = log -5
	ch = checkout
	pr = pull --rebase
	ci = commit
	re = rebase
	ss = stash save
	sp = stash pop
	sl = stash list
	df = diff
    lg = log --pretty=format:'%Cred%h %Creset%<(100,trunc)%s %Cgreen%ad %an %C(yellow)%d'
    lga = log --all --pretty=format:'%Cred%h %Creset%<(100,trunc)%s %Cgreen%ad %an %C(yellow)%d'
    lgg = log --graph --all --pretty=format:'%Cred%h %Creset%<(100,trunc)%s %Cgreen%ad %an %C(yellow)%d'
    lgdiff = log -p --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)'
	dfc = diff --cached
